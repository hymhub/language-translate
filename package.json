{
  "name": "language-translate",
  "version": "2.1.2",
  "description": "A plug-in that can translate ts/js/json multilingual files online and generate or insert specified files in batches",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hymhub/language-translate.git"
  },
  "author": "hym",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hymhub/language-translate/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "translate": "bin/translate.js"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/hymhub/language-translate#readme",
  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0",
    "baidu-fanyi": "^0.0.3",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "fast-glob": "^3.2.12",
    "inquirer": "^9.1.5",
    "tunnel": "^0.0.6",
    "typeof-plus": "^1.0.5",
    "unconfig": "^0.3.5"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.7.5",
    "@types/tunnel": "^0.0.3",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.3"
  },
  "keywords": [
    "translation",
    "translate",
    "google-translate",
    "language-translate",
    "i18n-json-translation"
  ]
}
